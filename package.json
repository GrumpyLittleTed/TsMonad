{
  "name": "tsmonad",
  "version": "0.8.0",
  "description": "TsMonad - fun-size monads library for TypeScript",
  "main": "./dist/tsmonad.js",
  "typings": "./lib/src/index",
  "files": [
    "dist",
    "lib/src/*.d.ts",
    "LICENSE-MIT",
    "README.md"
  ],
  "directories": {},
  "scripts": {
    "clean": "rm -rf dist lib",
    "prebuild": "npm run test",
    "build": "webpack --mode production lib/src/index.js -o dist/tsmonad.js -p --devtool source-map",
    "pretest": "npm run clean && tsc",
    "prepublish": "npm run build",
      "test": "mocha lib/test",
      "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbowdon/TsMonad.git"
  },
  "keywords": [
    "typescript",
    "monad",
    "functional"
  ],
  "author": {
    "name": "Chris Bowdon",
    "url": "https://github.com/cbowdon"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.7.7",
    "@types/underscore": "^1.9.4",
    "assert": "^2.0.0",
    "mocha": "^7.1.0",
    "typescript": "^3.8.3",
    "underscore": "^1.9.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
